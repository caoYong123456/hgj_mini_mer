}
.charge-center{
  text-align: center;
}
.van-cell{
  padding:16rpx 20rpx !important;
}
.van-collapse-item__content{
  padding: 0 !important;
}
/** map */
.map_popup_class{
  width: 90%;
  height:350px;
  border-radius: 8px;
}
.map-control-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background: #FFF;
  width: 100%;
}
.map-control-content {
  background: #FFF;
  padding: 20px 0 0;
  margin-top: 0px;
}
.map-container {
	position: relative;
	flex: 1;
	overflow: hidden;
  margin-top: 10px;
  width: 100%;
}
.introduce {
  padding: 0px 20px 10px 20px;
  text-align: center;
}
.map-panel {
	background: #fff;
}
.form-panel {
	background: #F8F8F7;
	flex: 1;
	overflow: hidden;
}
.map-content {
	position: absolute;
	z-index: 1;
  width: 100%;
  opacity: 0;
	height: 100%;
}
.map-content-show {
	display: flex;
	flex-direction: column;
	opacity: 1;
	z-index: 2;
  transition: 0.3s;
}
.map-padding {
  padding-bottom: 20px;
}
.map {
  width:100% important;
  height:235px;
}
.feature-switch {
  line-height: 51px;
}

.van-collapse-item-style{
  border-radius:.4em !important;
}

.collapse-item-index--van-cell.van-cell.van-cell--borderless{
  border-radius:.4em !important;
}
.charge-rule-tips-class{
  font-size: 24rpx !important;
  font-weight: 400;
  color: #999999;
  line-height: 32rpx !important;
  padding: 5rpx 20rpx;
}