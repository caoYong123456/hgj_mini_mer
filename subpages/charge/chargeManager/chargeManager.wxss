/** 充电列表 */
.charge-view{
  margin: -10rpx 30rpx;
}
.charge-area-view{
  margin: 8rpx 0rpx ;
  /**border-radius: 0.4em;
  box-shadow:rgba(0,0,0,0.07) 2rpx 2rpx 2rpx 2rpx ;**/
}
.van-cell{
  border-top-left-radius:0.4em !important;
  border-top-right-radius:0.4em !important;
}
.van-collapse-item__wrapper{
  border-radius: 0 0 0.4em 0.4em !important;
}
.van-cell .van-cell--clickable{
  border-radius: 0.4em 0.4em 0 0 !important;
}
.van-cell--borderless .van-cell--clickable{
  border-radius: 0.4em !important;
}
.van-hairline--top-bottom:after{
  border-top-width: 0px !important;
}
.van-cell__title{
  flex: 2 !important;
  color: #999999 !important;
  font-size: 28rpx !important;
}
.van-cell__value{
  flex: 4 !important;
  color: #363F4D !important;
  font-size: 28rpx !important;
  text-align: right !important;
}
.charge-rule-view .van-cell__value{
  text-align: left !important;
}
.charge-line{
  line-height: 80rpx;
}
.collapse-title-view{
  width: 100%;
}
.charge-title{
  color: #363f4d;
  font-weight: 500;
  font-size: 32rpx;
}
.charge-map-icon{
  padding-top: 6rpx;
  text-align: center;
  position: relative;
  top: 50%; /*偏移*/
}
.charge-map-btn{
  width: 120rpx !important;
  text-align: left;
  font-size: 28rpx;
  font-weight: 400;
  color: #258bff;
  position: relative;
  top: 50%; /*偏移*/
}
.charge-dev-view{
