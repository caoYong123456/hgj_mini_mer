page{
  height: 100%;
}

.empty-view{
  height: 100%;
  background-color: #fff;
}

.position-top{
  z-index:99;
  position:fixed;
  top:0px;
  height:120rpx;
  background-color:#fff;
  width: 100%;
  border-top:1px solid #ebedf0;
}

.ellipsis_view .van-col{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
}

.condition-view{
  font-size: 26rpx;
  color: #000;
  margin-left: 10rpx;
}

.check-condition-cell .van-cell__value{
  margin: 0 auto;
  height:80rpx;
  line-height: 80rpx;
  border-radius: 8rpx;
  border:2rpx solid #f2f2f2;
}

.cellRow0 {
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  padding: 0px 6px 0px 32rpx;
  background-color: white;
  height: 88rpx;
  line-height: 88rpx;
  justify-content: space-between;
}

.cellRow0 .right-container {
  display: flex;
  flex-direction: row;
  /* background-color: blue; */
  width: 80%;
  float: right;
  justify-content: flex-end;
}

.line {
  width: 100%;
  height: 1px;
  background-color: #f2f2f2;
}

.scroll-height {
  position: fixed;
  display:flex;
  height: calc(100% - 140rpx);
  top:100rpx;
  flex-direction: column;
  margin: 0rpx;
  padding: 0rpx;
}

.van-dropdown-menu {
  width: 480rpx !important;
  height: 88rpx !important;
  /* border: 1px solid #f2f2f2 !important;
  border-radius: 8rpx !important; */
  /* background-color: red !important; */
}

.van-dropdown-menu__title {
  padding-left: 0rpx !important;
  max-width: 90% !important;
  font-size: 28rpx !important;
  color: #363f4d !important;
}

.cell_titleClass {
  display: block;
  font-size: 28rpx;
  /* color: #777777; */
  padding: 0rpx 0rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.cell_titleClass_s {
  display: block;
  font-size: 28rpx;
  color: #1798dc;
  padding: 0rpx 0rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.van-dropdown-menu__title:after {
  color: #b2b8c2 !important;
  opacity: 1.0 !important;
}
.van-dropdown-item {
  margin-top: 1px !important;
}
.van-dropdown-menu__item{
  justify-content:flex-end !important;
  margin-right:40rpx;
}
.cellRow2 {
  display: flex;
  flex-direction: row;
  font-size: 14px;
  background-color: white;
  height: 88rpx;
  justify-content: center;
  align-items: center;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.queryButton {
  border-radius: 12rpx;
  height: 70rpx;
  width: 300rpx;
  color: white;
  background-color: #1798dc;
  text-align: center;
  line-height: 70rpx;
  margin: 10rpx;
  font-size: 32rpx;
}

.check-btn{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background-color: #1798dc;
  margin: 5rpx 40rpx 0rpx 0rpx;
  height: 52rpx;
  width: 120rpx;
  color: white;
  border: 1rpx solid #1798dc;
  border-radius: 8rpx;
}
.not-check-btn{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background-color: white;
  margin: 5rpx 40rpx 0rpx 0rpx;
  height: 52rpx;
  width: 120rpx;
  border: 1px solid #f2f2f2;
  border-radius: 8rpx;
}

.heoinfo-view {
  height: auto;
  margin-top:140rpx;
  flex-direction: column;
  width: 100%;
}

.fun-card{
  display: flex;
  flex-direction: column;
  justify-content: start;
  background: white;
  margin-top: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 3px;
  border: 1rpx solid #f2f2f2;
  box-shadow:none;
}

.van-hairline--top:after{
  border-top: none !important;
}

.click-hide-view{
  height:100%;
  background-color:rgba(0, 0, 0, 0.7); 
  transition-duration: 200ms;
  transition-property: opacity !important;
}