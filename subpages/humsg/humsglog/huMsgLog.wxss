/* subpages/humsg/humsglog/huMsgLog.wxss */
page{
  border-top: 0.5rpx solid #F2F2F2;
  font-family: Microsoft YaHei Regular, Microsoft YaHei Regular-Regular;
}

.page-container{
  background-color:#FFFFFF;
  margin: 20rpx 26rpx;
}

.no-data-container{
  margin: 40rpx 26rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #ababab;
}

.msgDetails-view{
  text-align: left;
  padding-left: 16rpx;
  padding-bottom: 10rpx;
  padding-top: 30rpx;
  background-color: #F7F7F7;
}
.msgDetails-text{
  font-size: 32rpx;
  color: #474747;
  font-weight: bold;
  line-height: 42rpx;
}

.huMsgLogC{
  padding: 10rpx 26rpx;
  border: 0.5rpx solid #F2F2F2;
  --margin-bottom: 10rpx;
  border-radius: 7rpx;
}

.msgTitleC .van-row{
  font-size: 28rpx;
  font-weight: 400;
  text-align: left;
  color: #474747;
  line-height: 70rpx;
  --border-bottom: 0.5rpx dashed #F2F2F2;
}

.msgBodyC .van-row{
  font-size: 28rpx;
  font-weight: 400;
  text-align: left;
  color: #474747;
  line-height: 50rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.msgSuccCntC .van-row{
  font-size: 28rpx;
  font-weight: 400;
  text-align: left;
  color: #474747;
  line-height: 70rpx;
}