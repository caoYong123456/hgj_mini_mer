.content{
  width: 258px;
  max-height: 600rpx;
  position: relative;
  top: 2rpx;
  left: 0rpx;
  background: #ffffff;
  box-shadow: 0px 0px 10px 1px rgba(209, 209, 209, 0.35);
  padding: 20rpx;
  overflow: hidden;
  overflow-y: scroll;
  border-radius: 10rpx;
  z-index: 100;
}
.content>view{
  padding: 10rpx 0;
}
.active{
  color: #56D88A;
}

  
