.head{
  text-align: center;
  padding-top: 370rpx;
}
.condition{
  padding: 10rpx 20rpx;
}
.last-month{
  font-family: Microsoft YaHei Regular;
  color: #282828;
  text-align: left;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  padding-left: 20rpx;
}
.curr-month{
  text-align: center;
  background-color: #FBF5E8;
