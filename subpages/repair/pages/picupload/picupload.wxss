/* pages/picupload.wxss */

.panel-title{
  margin-bottom: 20rpx;
  color:#C59C68
}
.ui-uploader-cell-other {
  width: 690rpx;
  padding: 40rpx 0;
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  flex-direction: row;
  /* justify-content: flex-start; *//* align-items: center; */
}
 
.ui-uploader-cell {
  width: 690rpx;
  /* padding: 20rpx 0; */
  display: flex;
  /* justify-content: flex-start; */
  flex-wrap: wrap;
  align-content: flex-start;
  flex-direction: row;
}
 
.ui-uploader-item {
  width: 150rpx;
  height: 150rpx;
  margin-right: 18rpx;
  margin-bottom: 18rpx;
  padding-bottom: 2rpx;
  padding-right: 2rpx;
  position: relative;
}
.ui-uploader-item-icon {
  position: absolute;
  right: 0;
}
.title2 {
  margin-top: 50rpx;
  width: 520rpx;
  display: flex;
  align-items: center;
  color:#C59C68
}
 

 
.ui-uploader {
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  color: #C59C68;
  background-color: rgba(255, 255, 255, 0.322);
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 0rpx 40rpx 0 rgba(0, 0, 0, 0.445);
}

.preimg{
  display: block; 
  box-sizing: border-box;
  height: 150rpx;
  width: 150rpx;
  margin:0;
}
