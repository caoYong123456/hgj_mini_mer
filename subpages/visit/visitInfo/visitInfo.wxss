/* pages/home/ownerManage/ownerManage.wxss */

page{
  height: 100%;
}

.position-top{
  z-index:333;
  position:fixed;
  top:0px;
  height:100rpx;
  background-color:#fff;
  width: 100%;
  border-top:1px solid #ebedf0;
}

.ellipsis_view .van-col{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
}

.ellipsis_view{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.check-condition-cell .van-cell__value{
  margin: 0 auto;
  height:60rpx;
  line-height: 60rpx;
  border-radius: 8rpx;
  border:2rpx solid #f2f2f2;
}

.cellRow0 {
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  padding: 0px 6px 0px 32rpx;
  background-color: white;
  height: 88rpx;
  line-height: 88rpx;
  justify-content: space-between;
}

.cellRow0 .right-container {
  display: flex;
  flex-direction: row;
  /* background-color: blue; */
  width: 80%;
  float: right;
  justify-content: flex-end;
}

.line {
  width: 100%;
  height: 1px;
  background-color: #f2f2f2;
}

.visit-list-view {
  width: 100%;
  height: 100%;
  flex-direction: column;
  margin-top: 104rpx;
}

.visit-srcoll-view{
  margin-top: 120rpx;
}

.van-dropdown-menu {
  width: 480rpx !important;
  height: 88rpx !important;
  /* border: 1px solid #f2f2f2 !important;
  border-radius: 8rpx !important; */
  /* background-color: red !important; */
}

.van-dropdown-menu__title {
  padding-left: 0rpx !important;
  max-width: 90% !important;
  font-size: 28rpx !important;
  color: #363f4d !important;
}
.van-dropdown-menu__title:after {
  color: #b2b8c2 !important;
  opacity: 1.0 !important;
}
.van-dropdown-item {
  margin-top: 1px !important;
}
.van-dropdown-menu__item{
  justify-content:flex-end !important;
  margin-right:40rpx;
}
.cellRow2 {
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  background-color: white;
  height: 88rpx;
  justify-content: center;
  align-items: center;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.queryButton {
  border-radius: 12rpx;
  height: 70rpx;
  width: 300rpx;
  color: white;
  background-color: #1798dc;
  text-align: center;
  line-height: 70rpx;
  margin: 10rpx;
  font-size: 32rpx;
}

.cellHouseInfo {
  display: flex;
  flex-direction: row;
  /* background-color: red; */
  width: 180rpx;
  margin-left: 220rpx;
}

.cell_titleClass {
  display: block;
  font-size: 28rpx;
  /* color: #777777; */
  padding: 0rpx 0rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.cell_titleClass_s {
  display: block;
  font-size: 28rpx;
  color: #1798dc;
  padding: 0rpx 0rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.loading-container {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #666666;
  margin-top: 6rpx;
  padding-bottom: 20rpx;
}

.visit-log-item{
  margin:20rpx 20rpx;
  background-color:#fff;
  border-radius: .5em;
}

.visit-log-name{
  flex: 7;
  text-align: left;
}

.visit-log-title{
  display: flex;
  margin:20rpx 10rpx 0px 10px;
  border-bottom: 1px dashed #D5D5D5;
  padding: 10rpx;
}

.visit-log-stat{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  font-size: 28rpx;
  color: #1798dc;
  flex: 2;
  text-align: right;
}

.visit-log-stat-hover{
  background-color: #f2f3f5;
  border-radius: .5em;
}

.blue_color{
  color: #189AFE;
}

.green_color{
  color:#14D164;
}

.grey_color{
  color: #DCDCDC;
}

.left-text{
  color:#666666;
  font-size: 28rpx;
}

.button-sp-area{
  display: flex;
  flex-direction: row;
}

.check-btn{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background-color: #1798dc;
  margin: 5rpx 40rpx 0rpx 0rpx;
  padding-top: 2rpx;
  height: 52rpx;
  width: 120rpx;
  color: white;
  border: 1rpx solid #1798dc;
  border-radius: 8rpx;
}
.not-check-btn{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background-color: white;
  /* border: solid rgb(241,241,241) 1rpx; */
  margin: 5rpx 40rpx 0rpx 0rpx;
  padding-top: 4rpx;
  height: 52rpx;
  width: 120rpx;
  border: 1px solid #f2f2f2;
  border-radius: 8rpx;
}

.jia-arrow-right{
  color: #b2b8c2;
  margin-left: 10rpx;
}

.van-cell-class{
  padding-bottom: 0rpx !important;
}